SELECT 
    CASE
        WHEN GRADES.GRADE >= 8 THEN STUDENTS.NAME
        ELSE 'NULL'
    END AS NAME,
    GRADES.GRADE, 
    STUDENTS.MARKs
FROM STUDENTS
JOIN GRADES
ON STUDENTS.MARKS BETWEEN GRADES.MIN_MARK AND GRADES.MAX_MARK
ORDER BY 
    GRADES.GRADE DESC,
    CASE
        WHEN GRADES.GRADE >= 8 THEN STUDENTS.NAME
        ELSE NULL
    END ASC,
    STUDENTS.MARKS ASC;